{
  "globals_path": "",
  "output_folder": "reports",
  "src_folders": ["tests"],
  "page_objects_path": ["page"],
  "launch_url": "http://localhost",
  "test_settings": {
    "default": {
      "screenshots": {
        "enabled": true,
        "on_failure": true,
        "path": "screenshots"
      }
    },
    "chrome": {
      "selenium_host": "chromedriver",
      "desiredCapabilities": {
        "browserName": "chrome"
      }
    },
    "firefox": {
      "selenium_host": "firefoxdriver",
      "desiredCapabilities": {
        "browserName": "firefox"
      }
    }
  }
}